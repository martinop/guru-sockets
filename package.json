    "url": "https://github.com/martinop/guru-sockets/issues"
  },
  "homepage": "https://github.com/martinop/guru-sockets#README",
  "dependencies": {
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "http": "0.0.0",
    "morgan": "^1.9.0",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "nodemon": "^1.11.0"
  }
